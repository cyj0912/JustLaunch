<Window x:Class="JustLaunch.SettingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="JustLaunch Settings" Height="480" Width="640" Icon="Tray.ico">
    <Grid>
        <Button x:Name="btnOK" Content="OK" HorizontalAlignment="Right" Margin="0,0,10,10" Width="75" VerticalAlignment="Bottom" Click="btnOK_Click"/>
        <Button x:Name="btnCancel" Content="Cancel" HorizontalAlignment="Right" Margin="0,0,95,10" VerticalAlignment="Bottom" Width="75" Click="btnCancel_Click"/>
        <Button x:Name="btnApply" Content="Apply" HorizontalAlignment="Right" Margin="0,0,180,10" VerticalAlignment="Bottom" Width="75" Click="btnApply_Click"/>
    </Grid>
</Window>
